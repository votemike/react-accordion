{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "ESNext",
    "lib": ["es6", "dom"],
    "moduleResolution": "nodenext",
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
  }
}
