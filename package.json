    "develop": "servatron -d dist & esbuild --watch --bundle demo/index.tsx --outfile=dist/index.min.js --loader:.js=jsx",
    "build": "esbuild --bundle src/index.tsx --outfile=dist/index.min.js --loader:.js=jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/votemike/react-accordion.git"
  },
  "keywords": [
